{
  "name": "@miharu/eslint-config-with-react",
  "version": "0.0.16",
  "description": "A set of ESLint rules",
  "main": "rules/.eslintrc.js",
  "scripts": {
    "build": "tsc",
    "clean": "jake clean",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "bin": {
    "init-eslint-config": "./bin/install.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MiharuCommunications/eslint-config-with-react"
  },
  "keywords": [
    "config",
    "eslint",
    "eslintconfig",
    "typescript"
  ],
  "files": [
    "LICENSE",
    "bin/**/*.js",
    "lib/**/*.js",
    "rules/**/*.js",
    "README.md"
  ],
  "author": "inahata",
  "license": "MIT",
  "dependencies": {
    "@miharu/eslint-config": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": "^3.8.0 || ^3.7.0 || ^3.6.0"
  },
  "devDependencies": {
    "@types/node": "^13.9.8",
    "del": "^5.1.0",
    "jake": "^10.4.6",
    "typescript": "^3.8.3"
  }
}
